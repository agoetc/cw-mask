{
    "manifest_version": 3,
    "name": "cw-mask",
    "version": "1.0.0.0",
    "content_scripts": [
        {
            "matches": ["https://www.chatwork.com/*"],
            "js": ["dist/main.js"]
        }
    ],
    "content_security_policy": {
        "script-src": "self",
        "object-src": "self"
    }
}
